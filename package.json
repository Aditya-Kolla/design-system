{
  "name": "design-system",
  "version": "0.0.0",
  "repository": "https://github.com/barnardos/design-system.git",
  "private": true,
  "scripts": {
    "check-format": "prettier '**/*.{css,json,js,md}' '!package.json' '!package-lock.json' '!.github/*.md' --list-different --ignore-path .gitignore",
    "format": "prettier '**/*.{css,json,js,md}' '!package.json' '!package-lock.json' '!.github/*.md' --write --ignore-path .gitignore",
    "lint:css": "stylelint '**/*.css' --max-warnings=0 --ignore-path .gitignore",
    "lint:md": "remark . --quiet --frail --rc-path .remarklintrc.json --ignore-path .gitignore",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "build-serve": "npm-run-all --sequential build serve",
    "pretest": "npm-run-all --parallel lint check-format",
    "test": "jest --ci",
    "watch-tests": "jest --watch --onlyChanged"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.17.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.4.3",
    "npm-run-all": "^4.1.2",
    "parse-css-font": "^2.0.2",
    "postcss-values-parser": "^1.5.0",
    "prettier": "1.10.2",
    "puppeteer": "^1.2.0",
    "remark-cli": "^5.0.0",
    "remark-preset-lint-itgalaxy": "^12.0.0",
    "stylelint": "^9.2.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-order": "^0.8.1",
    "stylelint-selector-bem-pattern": "^2.0.0"
  },
  "dependencies": {
    "gatsby": "next",
    "gatsby-plugin-postcss": "next",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^11.1.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-reporter": "^5.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "slugify": "^1.2.9"
  }
}
